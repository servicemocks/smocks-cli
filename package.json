{
  "name": "smocks-cli",
  "description": "Manage your servicemocks.com resources",
  "version": "1.0.0",
  "author": "Jeremy Gaerke",
  "bin": {
    "smocks-cli": "./bin/run"
  },
  "bugs": "https://github.com/servicemocks/smocks-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "app-module-path": "^2.2.0",
    "axios": "^0.19.0",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "globby": "^10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/servicemocks/smocks-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "smocks-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "https://github.com/servicemocks/smocks-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
